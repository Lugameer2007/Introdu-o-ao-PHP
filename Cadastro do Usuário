<?php

echo "<h1> Cadastro de Usuário</h1>";
echo "<hr>";

function CadastrarUsuario($nome, $idade, $email) {
    echo "Usuário cadastrado com sucesso!<br>";
    echo "Nome: $nome<br>";
    echo "Idade: $idade anos<br>";
    echo "E-mail: $email<br><br>";
}
CadastrarUsuario("Ana Paula", 28, "ana.paula@email.com");
CadastrarUsuario("Carlos Silva", 35, "carlos.silva@email.com");
